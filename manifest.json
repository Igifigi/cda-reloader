{
  "manifest_version": 2,
  "name": "CDA Reloader",
  "version": "0.1",
  "description": "Rozszerzenie odświeża stronę filmu cda w najwyższej możliwej jakości. ",
  "icons": {
    "16": "img/icon128.png",
    "48": "img/icon128.png",
    "128": "img/icon128.png"
  },
  "author": "Igifigi",
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
  "minimum_chrome_version": "16.0.884",
  "permissions": ["<all_urls>", "tabs"],
  "background": {
    "scripts": ["background.js"]
  }
}
